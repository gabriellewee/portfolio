---
title: Front-end Developer, Designer, and Creative
description: I’m a front‑end developer working at Apple .

layout: layouts/base.njk
key: home
style: true
isotope: true
add: true
tags: pages
message: Scroll to skip animation

pagination:
    data: collections.entries
    size: 20
    reverse: true
---

{% if pagination.pageNumber == 0 %}{% include 'pages/shared/links.html' %}{% endif %}

<section class="posts-container" aria-label="Posts">
	<div class="posts grid-isotope grid-scroll">
		{% for post in pagination.items %}
			{% if post.data.svg == true %}{%- set file = "svg" %}{% elseif post.data.extension %}{%- set file = post.data.extension %}{% endif %}
			<article
				class="post {% if pagination.pageNumber == 0 %} post-animate{% endif %}{% if (post.data.pagination.alias) and (post.data.pagination.alias != post.data.category) %} post-{{post.data.category}} post-{{post.data.pagination.alias}}{% else %} post-{{post.data.category}}{% endif %}{% if post.data.tags %}{% for tag in post.data.tags %}{% if tag != 'entries' %} post-{{tag}}{% endif %}{% endfor %}{% endif %}{% if post.data.size %} post-size-{{post.data.size}}{% endif %}{% if post.data.category == 'media' %} post-{{post.data.pagination.data}}{% endif %} grid-item gabrielle"
				{% if post.data.extension %}data-orientation="{% stats './static/images/' + post.data.pagination.data + '/' + post.data.date + '-' + post.data.name + '.' + file, 'orientation' %}"{% endif %}
				{% if post.data.width %}style="max-width: {{post.data.width}}"{% endif %}
				{% if post.data.category == "social" %}aria-label="{{post.data.pagination.alias|capitalize}} post"
				{% elseif post.data.category == "media" %}aria-label="{{post.data.type|capitalize}} post"{% endif %}
			>
				{% if post.data.category == "media" %}
					<input class="{{post.data.category}}-trigger" id="{{post.data.date|safe}}-{{post.data.name}}-info" type="checkbox" tabindex="0" aria-label="View information about this {{post.data.type}}" role="switch"/>
				{% endif %}
				<div class="post-inner">
					<header class="post-header{% if (post.data.category == 'writing') or (post.data.category == 'code') %} inline-header{% endif %}">
						<div class="post-header-content">
							{% if post.data.category == "writing" %}
								<h2>
									{% if post.url %}
										<a href="{{post.url|url}}"><span>{{post.data.title|safe}}</span></a>
									{% else %}
										<a href="{{post.data.url|url}}" target="_blank"><span>{{post.data.title|safe}}</span></a>
									{% endif %}
								</h2>
							{% elseif post.data.category == "code" %}
								<div class="post-header-left">
									<h2 class="post-headline">
										{% if post.data.uri %}
											<a href="https://codepen.io/gabriellewee/pen/{{post.data.uri}}" target="_blank"><span>{{post.data.title|safe}}</span></a>
										{% else %}
											{{post.data.title|safe}}
										{% endif %}
									</h2>
									{% if post.data.type %}
										<ul class="post-tags">
											{% for type in post.data.type %}
												<li><span class="tag-{{type}}">#{{type}}</span></li>
											{% endfor %}
										</ul>
									{% endif %}
								</div>
							{% endif %}
							{% if post.data.category == "code" %}
								<div class="post-header-right">
									<time class="time{% if post.data.category != 'code' %} time-default{% endif %}{% if post.data.category == 'social' %} time-external{% endif %}" datetime="{{post.page.date|ISO|date('string')}}" data-time="{{post.page.date|ISO}}" title="{{post.page.date|ISO|date('readable')}}">{{post.page.date|ISO|date('readable')}}</time>
									<a class="expand" id="{{post.page.date|ISO|date('string')}}-{{post.page.fileSlug}}" role="button" aria-expanded="false" aria-label="Expand preview of {{post.data.title}}" href="#{{post.page.date|ISO|date('string')}}-{{post.page.fileSlug}}-lightbox" data-desktop></a>
									<a class="external-expand" href="{{post.url|url}}" aria-label="View {{post.data.category}} in new window" target="_blank"></a>
								</div>
							{% elseif (post.data.category != "media") and (post.data.category != "code") %}
								<time class="time{% if post.data.category != 'code' %} time-default{% endif %}{% if post.data.category == 'social' %} time-external{% endif %}" datetime="{{post.page.date|ISO|date('string')}}" data-time="{{post.page.date|ISO}}" title="{{post.page.date|ISO|date('readable')}}">{{post.page.date|ISO|date('readable')}}</time>
							{% endif %}
						</div>
					</header>
					<div class="post-content{% if post.data.category == 'writing' %} post-text{% endif %}"{% if post.data.background %} style="background: {{post.data.background}}"{% endif %}>
						{% if post.data.category == "media" %}
							<figure id="{{post.data.date|safe}}-{{post.data.name}}" aria-label="{{post.data.type|capitalize}}{% if post.data.location %}, {{post.data.location}}{% elseif post.data.title %}, {{post.data.title}}{% endif %}" style="aspect-ratio: {% stats './static/images/' + post.data.pagination.data + '/' + post.data.date + '-' + post.data.name + '.' + file, 'ratio' %};">
								{% if post.data.upscale %}
									<picture>
										<img class="upscale" src="{% include post.data.upscale %}"{% if post.data.upscaleWidth %} width="{{post.data.upscaleWidth}}"{% endif %}{% if post.data.upscaleHeight %} height="{{post.data.upscaleHeight}}"{% endif %} alt="{{post.data.description}}"/>
									</picture>
								{% elseif post.data.svg == true %}
									<picture>
										<img src="/static/images/{{post.data.pagination.data}}/{{post.data.date}}-{{post.data.name}}.svg" alt="{{post.description}}" width="{% stats './static/images/' + post.data.pagination.data + '/' + post.data.date + '-' + post.data.name + '.svg', 'width' %}" height="{% stats './static/images/' + post.data.pagination.data + '/' + post.data.date + '-' + post.data.name + '.svg', 'height' %}"/>
									</picture>
								{% else %}
									{% image './static/images/' + post.data.pagination.data + '/' + post.data.date + '-' + post.data.name + '.' + post.data.extension, post.data.description, 'default', loop.index %}
								{% endif %}
								<label class="media-view-hover" for="{{post.data.date|safe}}-{{post.data.name}}-info" aria-hidden="true">
									<span class="button"><span><span>View info</span></span></span>
								</label>
								<label class="media-view-touch" for="{{post.data.date|safe}}-{{post.data.name}}-info" aria-hidden="true">View info</label>
								<figcaption class="{{post.data.category}}-data">
									<div class="media-data-wrapper">
										<time class="time-default" datetime="{{post.page.date|ISO|date('string')}}" title="{{post.page.date|ISO|date('readable')}}" data-time="{{post.page.date|ISO}}">{{post.page.date|ISO|date('readable')}}</time>
										<label class="media-down media-icon" for="{{post.data.date|safe}}-{{post.data.name}}-info"></label>
										<div class="media-thumbnail">
											<a class="media-expand expand" href="#{{post.data.date|safe}}-{{post.data.name}}-lightbox" role="button" aria-expanded="false" aria-label="Expand image" tabindex="-1">
												<span class="no-image">Expand</span>
												<span aria-hidden="true">
													{% if post.data.upscale %}
														<picture>
															<img class="upscale" src="{% include post.data.upscale %}"{% if post.data.upscaleWidth %} width="{{post.data.upscaleWidth}}"{% endif %}{% if post.data.upscaleHeight %} height="{{post.data.upscaleHeight}}"{% endif %} alt="{{post.data.description}}"/>
														</picture>
													{% elseif post.data.svg == true %}
														<picture>
															<img src="/static/images/{{post.data.pagination.data}}/{{post.data.date}}-{{post.data.name}}.svg" alt="{{post.description}}" width="{% stats './static/images/' + post.data.pagination.data + '/' + post.data.date + '-' + post.data.name + '.svg', 'width' %}" height="{% stats './static/images/' + post.data.pagination.data + '/' + post.data.date + '-' + post.data.name + '.svg', 'height' %}"/>
														</picture>
													{% else %}
														{% image './static/images/' + post.data.pagination.data + '/' + post.data.date + '-' + post.data.name + '.' + post.data.extension, post.data.description, 'thumbnail', loop.index %}
													{% endif %}
												</span>
											</a>
										</div>
										{% if post.data.external %}
											<div class="media-links">
												{% for link in post.data.external %}
													<a class="media-link external {{link.platform}}" target="_blank" href="{{link.url|url}}" tabindex="-1" aria-label="View on {{link.platform|capitalize}}"><span>{{link.platform|capitalize}}</span></a>
												{% endfor %}
											</div>
										{% endif %}
										<dl class="media-meta">
											{% if post.data.title %}
												<div class="meta-title">
													<dt>Title</dt><dd>{{post.data.title|nbsp|safe}}</dd>
												</div>
											{% endif %}
											{% if post.data.location %}
												<div class="meta-location">
													<dt>Location</dt><dd>{{post.data.location}}</dd>
												</div>
											{% endif %}
											{% if post.data.camera %}
												<div class="meta-camera">
													<dt>Camera</dt><dd>{{post.data.camera}}</dd>
												</div>
											{% endif %}
											{% if post.data.editor %}
												<div class="meta-editor">
													<dt>Editor</dt><dd>{{post.data.editor}}</dd>
												</div>
											{% endif %}
											{% if post.data.tools %}
												<div class="meta-tools">
													<dt>Tools</dt><dd>{{post.data.tools}}</dd>
												</div>
											{% endif %}
										</dl>
									</div>
								</figcaption>
							</figure>
						{% elseif post.data.category == "code" %}
							{% if post.data.button %}
								<button class="reload button"><span><span>{{post.data.button|capitalize}}</span></span></button>
							{% endif %}
							{% if post.url %}
								<iframe class="code{% for type in post.data.type %} {{type}}{% endfor %}" src="{{post.url|url}}" width="600" height="{{post.data.height}}" title="{{post.data.title|safe}}" loading="lazy" tabindex="-1" onload="this.style.opacity = 1;"{% if post.data.background %} style="background: {{post.data.background}}"{% endif %}></iframe>
								<noscript class="warning{% for type in post.data.type %} {{type}}{% endfor %}">Please enable Javascript to view this code demo.</noscript>
							{% else %}
								{{post.content|safe}}
							{% endif %}
						{% elseif post.data.content %}
							<blockquote>{{post.data.content|nbsp|safe}}</blockquote>
							{% if (post.data.category == "social") and (post.data.media) %}
								{% for media in post.data.media %}
									{% if media.url %}
										<figure id="external-{{media.id}}" class="media-attachment" aria-label="{{media.description}}">
											<a class="expand" href="#external-{{media.id}}-lightbox" role="button" aria-expanded="false" aria-label="Expand image">
												{% external media.url, media.description, 500 %}
											</a>
										</figure>
									{% endif %}
								{% endfor %}
							{% endif %}
						{% else %}
							{% excerpt post %}
						{% endif %}
					</div>
					{% if (post.data.category == "writing") or (post.data.category == "social") %}
						<footer class="post-footer">
							{% if post.data.category == "social" %}
								<a class="button button-social button-{{post.data.pagination.alias}}" href="{{post.data.url|url}}" aria-label="View post on {{post.data.pagination.alias|capitalize}}"><span><span>View post</span></span></a>
							{% elseif (post.data.category == "writing") and (post.data.size != "s") %}
								{% if post.url %}
									<a class="button" href="{{post.url|url}}"><span><span>Read post</span></span></a>
								{% elseif post.data.url %}
									<a class="button button-reverse" href="{{post.data.url|url}}" target="_blank"><span><span>Read more{% if post.data.platform %} at {{post.data.platform}}{% endif %}</span></a>
								{% endif %}
								{% for link in post.data.external %}
									<a class="button link-platform {{link.platform}} background" href="{{link.url|url}}" target="_blank"><span><span>{% if post.data.type == "audio" %}Listen on {% elseif post.data.type == "writing" %}Read on {% else %}View on {% endif %}{% if link.title %}{{link.title}}{% else %}{{link.platform|capitalize}}{% endif %}</span></a>
								{% endfor %}
							{% endif %}
							{% if post.data.archive == true %}
								<a class="button" href="{{site.url}}/v1/{{post.page.date|ISO|date('archive')}}/{{post.page.fileSlug}}" target="_blank"><span><span>View original</span></span></a>
							{% endif %}
						</footer>
					{% endif %}
				</div>
			</article>
		{% endfor %}
	</div>
	<nav class="pagination main-pagination" aria-label="Pagination">
		{% if (pagination.href.next != null) %}
			<button class="load button"><span><span>Load more</span></span></button>
		{% endif %}
		{% if (pagination.href.previous) and (pagination.pageNumber != 0) %}
			<a class="newer button button-reverse" href="{{pagination.href.previous}}"><span><span>Newer</span></span></a>
		{% endif %}
		{% if pagination.href.next %}
			<a class="older button" href="{{pagination.href.next}}"><span><span>Older</span></span></a>
		{% endif %}
	</nav>
</section>

{% include 'pages/posts/author.html' %}

<div class="lightbox-group posts-lightbox-group" hidden>
	{% for post in pagination.items %}
		{% if post.data.category == "media" %}
			<a class="lightbox" id="{{post.data.date|safe}}-{{post.data.name}}-lightbox" role="button" aria-expanded="true" aria-label="Close image" href="#{{post.data.date|safe}}-{{post.data.name}}" data-append></a>
			<figure class="image" role="dialog" aria-label="Image preview" aria-modal="true" data-content data-append>
				{% if post.data.upscale %}
					<picture>
						<img class="upscale" src="{% include post.data.upscale %}"{% if post.data.upscaleWidth %} width="{{post.data.upscaleWidth}}"{% endif %}{% if post.data.upscaleHeight %} height="{{post.data.upscaleHeight}}"{% endif %} alt="{{post.data.description}}"/>
					</picture>
				{% elseif post.data.svg == true %}
					<picture>
						<img src="/static/images/{{post.data.pagination.data}}/{{post.data.date}}-{{post.data.name}}.svg" alt="{{post.description}}" width="{% stats './static/images/' + post.data.pagination.data + '/' + post.data.date + '-' + post.data.name + '.svg', 'width' %}" height="{% stats './static/images/' + post.data.pagination.data + '/' + post.data.date + '-' + post.data.name + '.svg', 'height' %}"/>
					</picture>
				{% else %}
					{% image './static/images/' + post.data.pagination.data + '/' + post.data.date + '-' + post.data.name + '.' + post.data.extension, post.data.description, 'screen' %}
				{% endif %}
			</figure>
		{% elseif (post.data.category == "social") and (post.data.media) %}
			{% for media in post.data.media %}
				{% if media.url %}
					<a class="lightbox" id="external-{{media.id}}-lightbox" role="button" aria-expanded="true" aria-label="Close image" href="#external-{{media.id}}" data-append></a>
					<figure class="image" role="dialog" aria-label="Image preview" aria-modal="true" data-content data-append>
						{% external media.url, media.description, media.meta.original.width %}
					</figure>
				{% endif %}
			{% endfor %}
		{% elseif (post.data.category == "writing") %}
			{% excerpt post, "lightbox" %}
		{% elseif post.data.category == "code" %}
			<a class="lightbox" id="{{post.page.date|ISO|date('string')}}-{{post.page.fileSlug}}-lightbox" role="button" aria-expanded="true" aria-label="Close preview of {{post.data.title}}" href="#{{post.page.date|ISO|date('string')}}-{{post.page.fileSlug}}" data-append data-desktop></a>
			<figure class="iframe" role="dialog" aria-label="Preview of {{post.data.title}}" aria-modal="true" data-append data-content>
				{% if post.url %}
					<iframe class="code{% for type in post.data.type %} {{type}}{% endfor %}" src="{{post.url|url}}" width="600" height="{{post.data.height}}" title="{{post.data.title}}" loading="lazy" tabindex="-1" onload="this.style.opacity = 1;"{% if post.data.background %} style="background: {{post.data.background}}"{% endif %}></iframe>
					<noscript class="warning{% for type in post.data.type %} {{type}}{% endfor %}">Please enable Javascript to view this code demo.</noscript>
				{% else %}
					{{post.content|safe}}
				{% endif %}
			</figure>
		{% endif %}
	{% endfor %}
	<div class="lightbox-background" data-append></div>
</div>