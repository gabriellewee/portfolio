<!doctype html>
<html lang="en" class="no-js">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, viewport-fit=cover">
	<meta name="format-detection" content="telephone=no">

	{% if hidden == true %}<meta name="robots" content="noindex,nofollow">{% endif %}

	<link rel="canonical" href="{{site.url}}{{page.url}}">
	
	<link rel="mask-icon" href="/static/images/favicons/safari-pinned-tab.svg" color="#F5C8BF">
	<link rel="icon" type="image/png" sizes="32x32" href="/static/images/favicons/favicon-32x32.png">
	<link rel="icon" type="image/png" sizes="16x16" href="/static/images/favicons/favicon-16x16.png">
	<link rel="shortcut icon" href="/static/images/favicons/favicon.ico">
	<link rel="shortcut icon" type="image/svg+xml" href="/static/images/favicons/favicon.svg">
	<link rel="apple-touch-icon" sizes="180x180" href="/static/images/favicons/apple-touch-icon-180x180.png">
	<link rel="manifest" href="/static/images/favicons/manifest.webmanifest">

	<meta name="theme-color" content="#fcf1ef">
	<meta name="application-name" content="{{site.author}}">
	<meta name="description" content="{{description}}">
	<meta name="msapplication-config" content="static/images/favicons/browserconfig.xml">

	<meta name="theme-color" content="#fcf1ef" media="(prefers-color-scheme: light)">
	<meta name="theme-color" content="#263238" media="(prefers-color-scheme: dark)">

	<title>{{title}} · {{site.author}}</title>
	<meta property="og:title" content="{{title}}">

	<meta property="og:description" content="{{description}}">
	<meta property="og:site_name" content="{{site.author}}">
	<meta property="og:type" content="website"/>
	<meta property="og:url" content="{{site.url}}{{permalink}}">
	<meta property="og:image" content="{{site.url}}/static/images/og/{% if ogImage %}{{ogImage}}{% else %}default{% endif %}.jpg">
	<meta property="og:image:type" content="image/jpg">
	<meta property="og:image:width" content="1200">
	<meta property="og:image:height" content="630">
	
	<meta name="twitter:title" content="{{title}}">
	<meta name="twitter:card" content="summary_large_image">
	<meta name="twitter:site" content="@gabriewee">
	<meta name="twitter:creator" content="@gabriewee">
	<meta name="twitter:description" content="{{description}}">
	<meta property="twitter:image" content="{{site.url}}/static/images/og/{% if ogImage %}{{ogImage}}{% else %}default{% endif %}.jpg">

	<meta property="fb:app_id" content="1291587600924107">
	<meta property="fb:admins" content="504790226">

	<meta property="article:author" content="{{site.author}}">

	<link rel="stylesheet" href="{% if customStyles %}{{ '/static/css/' + customStyles + '.css' | url }}{% else %}{{'/static/css/style.css' | url}}{% endif %}">

	<noscript>
		<link rel="stylesheet" href="{{'/static/css/no-js.css' | url}}">
	</noscript>
</head>
<body{% if templateClass %} class="page-{{templateClass}}"{% endif %}>
	{% if headerLink == true %}<header class="top" aria-label="Header"{% if home %} aria-hidden="true"{% endif %}>
		<a class="logo" href="/" aria-label="Go home?"{% if home %} tabindex="-1"{% endif %}>
			<picture class="memoji memoji-default">
				<source srcset="/static/images/avatars/memoji.webp, /static/images/avatars/memoji@2x.webp 2x" type="image/webp">
				<source srcset="/static/images/avatars/memoji.png, /static/images/avatars/memoji@2x.png 2x" type="image/png">
				<img src="/static/images/avatars/memoji.png" alt="" width="48px" height="68px">
			</picture>
			<picture class="memoji memoji-hm">
				<source srcset="/static/images/avatars/memoji-hm.webp, /static/images/avatars/memoji-hm@2x.webp 2x" type="image/webp">
				<source srcset="/static/images/avatars/memoji-hm.png, /static/images/avatars/memoji-hm@2x.png 2x" type="image/png">
				<img src="/static/images/avatars/memoji-hm.png" alt="" width="48px" height="68px">
			</picture>
		</a>
		<span class="go-home" aria-hidden="true">Go home?</span>
		{% if navMessage == true %}
			<span class="message">{{description}}</span>
		{% elseif navMessage %}
			<span class="message">{{navMessage}}</span>
		{% endif %}
	</header>{% endif %}
	<main>
		{% block content %}{{content | safe}}{% endblock %}
	</main>
	<footer class="bottom" aria-label="Footer">
		<p class="copyright">
			{% if aboutLink == true %}<a href="/about/">About</a>{% endif %}
			&copy; {{site.author}} {% year 'start' %}–{% year 'current' %}
		</p>
	</footer>
	{% if siteScripts == true %}<script src="{{'/static/js/isotope.min.js' | url}}"></script>
	<script src="{{'/static/js/clipboard.min.js' | url}}"></script>
	<script src="{{'/static/js/packery.min.js' | url}}"></script>
	<script src="{{'/static/js/imagesloaded.min.js' | url}}"></script>
	<script src="{{'/static/js/smoothscroll.min.js' | url}}"></script>
	<script src="{{'/static/js/lightbox.js' | url}}"></script>
	<script src="{{'/static/js/main.js' | url}}"></script>
	<script>document.body.addEventListener('touchstart', function() {}, {passive: true});</script>{% endif %}
</body>
</html>
